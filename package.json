    "eslint-plugin-react": "^7.7.0",
    "jest": "^22.4.2",
    "jest-enzyme": "^6.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
